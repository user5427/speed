{
  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fontsource/fredoka": "^5.1.0",
    "@mui/icons-material": "^6.1.5",
    "@testing-library/user-event": "^13.5.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "axios": "^1.7.7",
    "bootstrap": "^5.3.3",
    "country-flag-icons": "^1.5.13",
    "i18next-http-backend": "^2.6.2",
    "primereact": "^10.8.3",
    "react": "^18.3.1",
    "react-app": "file:",
    "react-bootstrap": "^2.10.5",
    "react-dom": "^18.3.1",
    "react-dom-confetti": "^0.2.0",
    "react-i18next": "^15.1.0",
    "react-icons": "^5.3.0",
    "react-paginate": "^8.2.0",
    "react-router-dom": "^6.27.0",
    "react-scripts": "5.0.1",
    "react-select": "^5.8.1",
    "styled-components": "^6.1.13",
    "typescript": "^5.6.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/setupTests.js",
      "!src/serviceWorker.js",
      "!src/**/*.test.{js,jsx}",
      "!src/test-i18n.js",
      "!src/reportWebVitals.js",
      "!src/.components/.common-components/**",
      "!src/.constants/**",
      "!src/.controllers/**",
      "!src/.helpers/**",
      "!src/.entities/**",
      "!src/pages/Exercise/articleData.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statments": -10
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/react-i18next": "^7.8.3",
    "i18next-fs-backend": "^2.3.2",
    "jest": "^27.5.1"
  }
}
